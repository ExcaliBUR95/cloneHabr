{
  "author": "Steve Kellock",
  "bin": {
    "osnap": "bin/osnap"
  },
  "dependencies": {
    "execa": "^0.6.3",
    "minimist": "^1.2.0",
    "pify": "^3.0.0",
    "tempfile": "^2.0.0",
    "which": "^1.2.14"
  },
  "description": "A CLI for grabbing ios/android screenshots and saving to your clipboard or filesystem.",
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/node": "^18.11.13",
    "@types/pify": "^0.0.28",
    "@types/tempfile": "^1.1.5",
    "@types/which": "^1.0.28",
    "np": "^7.6.2",
    "prettier": "^1.4.2",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.2",
    "typescript": "^4.9.4"
  },
  "files": [
    "bin/osnap",
    "macos/pbcopyimg",
    "README.md",
    "src/*.js"
  ],
  "keywords": [
    "ios",
    "android",
    "screenshot"
  ],
  "license": "MIT",
  "name": "@ferocia-oss/osnap",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ferocia/osnap.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "build:mac": "gcc -Wall -O3 -ObjC -framework Foundation -framework AppKit -o macos/pbcopyimg macos/pbcopyimg.m",
    "clean": "rimraf {bin,src}/**/*.{d.ts,js}",
    "pretty": "prettier --no-semi --single-quote --print-width 100 --parser typescript --write src/**/*.ts",
    "watch": "tsc -p . -w",
    "test": "echo \"Error: no test specified\"",
    "release": "np"
  },
  "version": "1.3.0"
}
